<%= @intern.nome %><br />
Pediatria

<%= form_for([@intern, @intern.firstyear, @intern.firstyear.fypediatry]) do |f| %>
	<fieldset>
		<legend>Editar Pediatria</legend>
		<%= f.label :supervisor_id %>
		<%= f.collection_select :supervisor_id, Supervisor.all, :id, :nome %>
		<%= f.label :data_inicio %>
		<%= f.date_select :data_inicio %>
		<%= f.label :data_fim %>
		<%= f.date_select :data_fim %>
		<%= f.label :nota %>
		<%= f.text_field :nota %>
	</fieldset>
	<%= f.submit 'Guardar', class: 'btn btn-success' %>		
<% end %>