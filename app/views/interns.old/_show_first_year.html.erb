<h2>Ano Comum</h2>

<div class="span10 offset1">
	<%= link_to "Editar", edit_intern_firstyear_path(@intern, @intern.firstyear), class: 'btn' %>
	<%= link_to "Eliminar", [@intern, @intern.firstyear], confirm: 'Eliminar Ano Comum?', method: :delete, class: 'btn btn-danger' %>
	<table class="table">
		<thead>
			<tr>
				<th>Ano</th>
				<th>Local</th>
				<th>Observações</th>
			</tr>
		</thead>
		<tbody>
			<tr>
				<td><%= @intern.firstyear.ano %></td>
				<td><%= @intern.firstyear.local %></td>
				<td><%= @intern.firstyear.observacoes %></td>
			</tr>
		</tbody>
	</table>
</div>

<div class="row-fluid">
	<div class="span12">
		<table class="table table-bordered">

			<tbody>
				<tr>
					<td><%= render('fymedicine') %></td>
					<td><%= render('fyprimary') %></td>
					<td><%= render('fycirurgy') %></td>
					<td><%= render('fypediatry') %></td>
					<td><%= render('fychoice') %></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>