<tr>
	<% unless @intern.firstyear.fyprimary.nil? %>
		<td><%= link_to "Primários", edit_intern_firstyear_fyprimary_path(@intern, @intern.firstyear, @intern.firstyear.fyprimary) %></td>
		<% unless @intern.firstyear.fyprimary.healthcareunit.nil? %>
			<td><%= @intern.firstyear.fyprimary.healthcareunit.nome %></td>
		<% else %>
			<td></td>
		<% end %>
		<% unless @intern.firstyear.fyprimary.supervisor.nil? %>
			<td><%= @intern.firstyear.fyprimary.supervisor.nome %></td>
		<% else %>
			<td>&nbsp;</td>
		<% end %>
		<td><%= @intern.firstyear.fyprimary.data_inicio %></td>
		<td><%= @intern.firstyear.fyprimary.data_fim %></td>
		<td><%= @intern.firstyear.fyprimary.nota %></td>
	<% else %>
		<%= link_to "Cuidados Primários", new_intern_firstyear_fyprimary_path(@intern, @intern.firstyear), class: 'btn btn-primary' %>
	<% end %>
</tr>