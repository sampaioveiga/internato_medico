<% @internships.each do |internship| %>
	<%= internship.nome %><br />
	<%= internship.supervisor.nome %><br />
	<%= internship.healthcareunit.nome %><br />
	<%= internship.service.nome %><br />
	<%= internship.estado %><br />
	<%= internship.data_inicio %><br />
	<%= internship.data_fim %><br />
	<%= internship.nota %><br />
	<%= internship.observacoes %><br />
	<%= link_to "Editar Especialidade", edit_intern_internship_path(@intern, internship) %>
	<%= link_to 'Eliminar Especialidade', [@intern,internship], method: :delete, data: { confirm: 'Tem a certeza?' } %>
<% end %>