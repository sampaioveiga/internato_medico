<%= @intern.firstyear.ano %><br />
<%= link_to "Editar Ano Comum", edit_intern_firstyear_path(@intern, @intern.firstyear) %><br />
<%= link_to "Eliminar Ano Comum", [@intern, @intern.firstyear], method: :delete, data: { confirm: 'Are you sure?' } %><br />

<% unless @intern.firstyear.fymedicine.nil? %>
	Medicina<br />
	<%= @intern.firstyear.fymedicine.supervisor.nome %><br />
	<%= @intern.firstyear.fymedicine.data_inicio %><br />
	<%= @intern.firstyear.fymedicine.data_fim %><br />
	<%= @intern.firstyear.fymedicine.nota %><br />
	<%= link_to "Editar Medicina", edit_intern_firstyear_fymedicine_path(@intern, @intern.firstyear, @intern.firstyear.fymedicine) %>
	<%= link_to "Eliminar Medicina", [@intern, @intern.firstyear, @intern.firstyear.fymedicine], method: :delete, data: { confirm: 'Are you sure?' } %><br />
<% else %>
	<%= form_for([@intern, @intern.firstyear, @intern.firstyear.build_fymedicine]) do |f| %>
		<fieldset>
			<legend>Medicina</legend>
			<%= f.label :supervisor_id %>
			<%= f.collection_select :supervisor_id, Supervisor.all, :id, :nome %>
			<%= f.label :data_inicio %>
			<%= f.date_select :data_inicio %>
			<%= f.label :data_fim %>
			<%= f.date_select :data_fim %>
			<%= f.label :nota %>
			<%= f.text_field :nota %>
		</fieldset>
		<%= f.submit %>
	<% end %>
<% end %>

