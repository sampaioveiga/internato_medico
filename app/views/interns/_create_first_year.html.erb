<%= form_for([@intern, @intern.build_firstyear]) do |f| %>
	<fieldset>
		<legend>Ano Comum</legend>
		<%= f.label :ano %>
		<%= f.text_field :ano %>
		<%= f.label :local %>
		<%= f.text_field :local %>
		<%= f.label :observacoes %>
		<%= f.text_area :observacoes %>
	</fieldset>
	<%= f.submit %>
<% end %>